{
  "name": "trevordmiller-website",
  "scripts": {
    "upgrade": "npm-check -u && npm audit fix",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./",
    "dev": "next",
    "precommit": "lint-staged",
    "verify": "npm run lint && npm run test && npm run broken-links && npm run build && npm run size",
    "lint": "eslint .",
    "format": "prettier --write '**/*.{js,json,md}'",
    "test": "jest",
    "test:debug": "DEBUG=true npm run test",
    "broken-links": "blc ${URL:=http://localhost:3000} -ro --exclude=linkedin.com --exclude=visualstudio.com --exclude=netflix.com",
    "build": "next build",
    "size": "bundlesize -f './.next/static/commons/main-*.js' -s 150kB",
    "start": "next start"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@mdx-js/loader": "^0.14.0",
    "@mdx-js/mdx": "^0.14.0",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "broken-link-checker": "^0.7.8",
    "bundlesize": "^0.17.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "now": "^11.3.0",
    "npm-check": "^5.7.1",
    "prettier": "^1.13.7",
    "puppeteer": "^1.6.0",
    "react-test-renderer": "^16.4.1",
    "regenerator-runtime": "^0.12.0",
    "remark-highlight.js": "^5.0.0"
  },
  "dependencies": {
    "next": "^6.1.1",
    "nova-colors": "^2.1.5",
    "prop-types": "^15.6.2",
    "raven-js": "^3.26.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-lazyload": "^2.3.0",
    "rss": "^1.2.2"
  },
  "engines" : { 
    "node" : ">=8.0",
    "npm" : ">=6.0" 
  },
  "browser": {
    "rss": false
  }
}
