{
  "name": "trevordmiller-website",
  "scripts": {
    "upgrade": "npm-check -u && npm audit fix",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "dev": "next",
    "precommit": "lint-staged",
    "verify": "npm run lint && npm run test && npm run unused && npm run build && npm run size",
    "lint": "eslint .",
    "format": "prettier --write '**/*.{js,json,md}'",
    "test": "jest",
    "unused": "depcheck --ignores='utils,components,pages,husky,eslint-*,@babel/*,@mdx-js/*'",
    "build": "next build",
    "size": "bundlesize -f './.next/static/commons/main-*.js' -s 150kB",
    "deploy": "now",
    "alias": "now alias && blc https://trevordmiller.com -ro",
    "start": "next start"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.11.0",
    "@mdx-js/mdx": "^0.10.1",
    "@storybook/react": "^3.4.8",
    "babel-eslint": "^8.2.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "broken-link-checker": "^0.7.8",
    "bundlesize": "^0.17.0",
    "depcheck": "^0.6.9",
    "eslint": "^5.0.0",
    "eslint-import-resolver-babel-module": "^5.0.0-beta.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "jest": "^23.1.0",
    "lint-staged": "^7.2.0",
    "now": "^11.2.4",
    "npm-check": "^5.7.1",
    "prettier": "^1.13.5",
    "remark-highlight.js": "^5.0.0"
  },
  "dependencies": {
    "next": "^6.0.3",
    "nova-colors": "^2.1.5",
    "prop-types": "^15.6.2",
    "raven-js": "^3.26.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rss": "^1.2.2"
  },
  "browser": {
    "rss": false
  }
}
