{
  "name": "trevordmiller-website",
  "scripts": {
    "upgrade": "npm-check -u && npm audit fix",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./",
    "dev": "next",
    "precommit": "lint-staged",
    "format": "prettier --write '**/*.{js,json,md}'",
    "verify": "npm run lint && npm run test && npm run build && npm run size",
    "lint": "eslint .",
    "test": "jest utils/ components/",
    "build": "next build",
    "size": "bundlesize -f './.next/static/commons/main-*.js' -s 150kB",
    "verify-url": "npm run test-e2e && npm run broken-links",
    "test-e2e": "jest e2e/",
    "test-e2e:debug": "DEBUG=true npm run test-e2e",
    "broken-links": "blc ${APP_URL:=http://localhost:3000} -ro --exclude=linkedin.com --exclude=visualstudio.com --exclude=netflix.com",
    "start": "next start"
  },
  "devDependencies": {
    "@mdx-js/loader": "^0.9.0",
    "@mdx-js/mdx": "^0.9.0",
    "@storybook/addon-info": "^3.4.8",
    "@storybook/addon-options": "^3.4.8",
    "@storybook/react": "^3.4.8",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "broken-link-checker": "^0.7.8",
    "bundlesize": "^0.17.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.2.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.18.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "husky": "^0.14.3",
    "imagemin-lint-staged": "^0.3.0",
    "jest": "^23.4.2",
    "lint-staged": "^7.2.0",
    "now": "^11.2.4",
    "npm-check": "^5.7.1",
    "prettier": "^1.14.0",
    "puppeteer": "^1.6.1",
    "react-test-renderer": "^16.4.1",
    "regenerator-runtime": "^0.12.0",
    "remark-highlight.js": "^5.0.0"
  },
  "dependencies": {
    "next": "^5.1.0",
    "nova-colors": "^2.1.5",
    "prop-types": "^15.6.2",
    "raven-js": "^3.26.4",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-lazyload": "^2.3.0",
    "rss": "^1.2.2",
    "styled-components": "^3.3.3"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "6.x.x"
  },
  "browser": {
    "rss": false
  }
}
